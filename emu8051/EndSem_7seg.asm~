	ORG	0H
	LJMP	100H		;bypass IVT

	ORG	50H
	DB	3FH, 06H, 5BH, 4FH, 66H, 6DH, 7DH, 7H, 7FH, 6FH, 77H, 7CH, 39H, 5EH, 79H, 71H
; Lookup table for digits 0 to 9
	
	ORG	100H

START:	MOV	R7, #16
	MOV	P0, #0
	MOV	DPTR, #50H

BACK:	CLR	A
	MOVC	A, @A+DPTR
	MOV	P0, A
	INC	DPTR
	ACALL	DELAY
	DJNZ	R7, BACK
	SJMP	START

DELAY:	MOV	R1, #5
WAIT1:	MOV	R2, #25
WAIT:	DJNZ	R2, WAIT